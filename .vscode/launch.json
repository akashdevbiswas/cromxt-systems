{
    "configurations": [
        {
            "type": "java",
            "name": "CrombucketGatewayApplication",
            "request": "launch",
            "mainClass": "com.crombucket.gateway.crombucket_gateway.CrombucketGatewayApplication"
        },
        {
            "type": "java",
            "name": "StorageManagerApplicationMain",
            "request": "launch",
            "mainClass": "com.crombucket.storagemanager.StorageManagerApplicationMain",
            "projectName": "storage-manager"
        },
        {
            "type": "java",
            "name": "Spring Boot-CromxtConfigServer<cromxt-configserver>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.cromxt.configserver.CromxtConfigServer",
            "projectName": "cromxt-configserver",
            "args": "",
            "envFile": "${workspaceFolder}/.env",
            "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=33723 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=cromxt-configserver"
        },
        {
            "type": "java",
            "name": "Spring Boot-UserServiceMain<user-service>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.cromxt.auth.UserServiceMain",
            "projectName": "user-service",
            "args": "",
            "envFile": "${workspaceFolder}/.env",
            "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=46301 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=user-service"
        },
        {
            "type": "java",
            "name": "Spring Boot-ClusterManagerApplication<storagenode-router>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.crombucket.clusterrouter.ClusterManagerApplication",
            "projectName": "storagenode-router",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "type": "java",
            "name": "Spring Boot-CrombucketGatewayApplication<crombucket-gateway>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.crombucket.gateway.crombucket_gateway.CrombucketGatewayApplication",
            "projectName": "crombucket-gateway",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
