{
  "configurations": [
    {
      "type": "java",
      "name": "Spring Boot-CromxtConfigServer<config-server>",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "com.cromxt.configserver.CromxtConfigServer",
      "projectName": "config-server",
      "args": "",
      "envFile": "${workspaceFolder}/.env",
      "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=45875 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=config-server"
    },
    {
      "type": "java",
      "name": "Spring Boot-UserServiceMain<user-service>",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "com.cromxt.auth.UserServiceMain",
      "projectName": "user-service",
      "args": "",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "java",
      "name": "Spring Boot-StorageManagerApplicationMain<storage-manager>",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "com.crombucket.storagemanager.StorageManagerApplicationMain",
      "projectName": "storage-manager",
      "args": "",
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}